    "preinstall": "npx npm-force-resolutions",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.2.2",
    "posthtml-expressions": "^1.7.1",
    "posthtml-include": "^1.6.0",
    "sass": "^1.26.5"
  },
  "dependencies": {
    "enter-view": "^2.0.0",
    "intersection-observer": "^0.10.0",
    "text-balancer": "^1.0.5"
  },
  "resolutions": {
    "browserslist": "4.14.2",
    "caniuse-lite": "1.0.30001129"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "spectate": {
    "USE_NEWS_NAV": false,
    "USE_EYE_NAV": true,
    "USE_COVER_HED": true,
    "IS_EMBED": false,
    "DOC_URL": "https://docs.google.com/document/d/1pb5_YWij2pRI0xkCU4_cZQiUy10raVI_NrVxJkcp5sU/edit?usp=sharing"
