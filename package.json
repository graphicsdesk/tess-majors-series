{
  "name": "tess-majors-series",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --global script",
    "build": "parcel build src/index.html src/index1.html src/index2.html --global script --public-url https://spectator-static-assets.s3.amazonaws.com/tess-majors-series --no-content-hash --no-source-maps",
